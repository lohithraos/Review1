class student {
    void show() { System.out.println("student's show())
}
 
// Inherited class
class Child extends {
    // This method overrides show() of student
    @Override void sho()
    {
        System.out.println( Child's show());
    }
}
 
// Driver class
class Main {
    public static void main(String[] args)
    {
        // If a student type reference refers
        // to a student object, then Parent's
        // show is called
        student obj1 = new student()
        obj1.show();
 
        // If a Parent type reference refers
        // to a Child object Child's show()
        // is called. This is called RUN TIME
        // POLYMORPHISM.
        student obj2 = new Child()
        obj.show();
    }
}
